<UIElements name="Dot">
  <UIElement name="Dot">  
    <GFx file="dot.swf" layer="10">
      <Constraints>
        <Align mode="dynamic" halign="centre" valign="centre"/>
      </Constraints>
    </GFx> 
    <functions>
		<function name="PlayMessage" funcname="playMessage">
			<param name="message" desc="Message which will be showed" type="string"/>
		</function>
		<function name="MouseOut" funcname="onMouseOut"/>
	</functions>
  </UIElement>
</UIElements>


